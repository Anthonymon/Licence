let
  
  function read_unsigned() : int =
  let
	var str : string := getchar()
	var nb : int :=0
	var passage : int :=1
  in
	if(str="\n") then passage:=0;
	while(str<>"\n") do
		if((ord(str)>=ord("0"))&(ord(str)<=ord("9"))) then
			(nb:= nb*10+ord(str)-48; str:=getchar())
		else
			(nb:=-1; break);
	if(passage=0) then nb:=-1;
	nb
   end 
  var a : int := read_unsigned()
in
  print_int(a*2);
  print("\n")
end
